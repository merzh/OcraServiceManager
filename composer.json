{
    "name": "ocramius/ocra-service-manager",
    "description": "ZF2 Service manager extensions to allow service proxies and dependency graph generation",
    "type": "library",
    "license": "MIT",
    "tags": [
        "zend framework",
        "lazy loading",
        "module",
        "servicemanager"
    ],
    "homepage": "https://github.com/Ocramius/OcraServiceManager",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "zendframework/zend-mvc":             "2.1.*",
        "zendframework/zend-servicemanager":  "2.1.*",
        "zendframework/zend-modulemanager":   "2.1.*",
        "doctrine/common":                    ">=2.4.*@dev,<2.5.*@dev"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "dev-master"
    },
    "suggest": {
        "zendframework/zend-developer-tools": "Allows displaying service dependencies"
    },
    "autoload": {
        "psr-0": {
            "OcraServiceManager\\": "src"
        }
    }
}
